{
  "name": "react-native-widget-extension-example",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "doc": "bun x expo-doctor@latest",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --platform ios --clean",
    "build:simulator": "eas build --profile simulator --platform ios --local",
    "build:development": "eas build --profile development --platform ios --local"
  },
  "dependencies": {
    "expo": "~54.0.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "expo-dev-client": "~6.0.0",
    "expo-splash-screen": "~31.0.0",
    "expo-status-bar": "~3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "typescript": "^5.1.3"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
